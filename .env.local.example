# Data Source Configuration
# Set to 'true' to load from S3, 'false' to load from public folder
NEXT_PUBLIC_USE_S3=false

# Local File Paths (relative to public/ folder when USE_S3=false)
NEXT_PUBLIC_CSV_PATH=/data/dashboard_output.csv
NEXT_PUBLIC_JSON_PATH=/data/model.json

# S3 Configuration (only needed when USE_S3=true)
NEXT_PUBLIC_S3_BUCKET=your-bucket-name
NEXT_PUBLIC_AWS_REGION=us-east-1
NEXT_PUBLIC_AWS_ACCESS_KEY_ID=your-access-key
NEXT_PUBLIC_AWS_SECRET_ACCESS_KEY=your-secret-key